<%= simple_form_for(tournament) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :number_of_teams %>
  <%= f.input :number_of_players_per_team %>
  <%= f.input :price %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.submit %>
<% end %>
