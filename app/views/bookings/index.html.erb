<h1>Bookings#index</h1>
<p>Find me in app/views/bookings/index.html.erb</p>
<% if @tournament.user_id == current.user %>
<% @bookings.each do |booking| %>
    <%= booking.tournament.name %>
    <%= booking.tournament.location %>
    <%= booking.tournament.start_time %>
    <%= booking.tournament.end_time %>
    <%= link_to "Tournament Info",tournament_path(booking.tournament) %>
<% end %>
